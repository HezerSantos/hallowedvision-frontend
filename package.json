{
  "name": "ts-practice",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "commit": "sh -c 'git add -A && git commit -m \"$0\" && git push'",
    "start": "npx serve -s ./dist",
    "dev": "vite",
    "build": "tsc -b && vite build --mode production && npm run obfuscate",
    "obfuscate": "find dist -type f -name '*.js' -exec javascript-obfuscator {} --output {} --compact true --self-defending true --control-flow-flattening true --rename-globals true \\;",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "mobile": "npx ngrok http 5173",
    "fix": "find .git/objects/ -type f -empty | xargs rm; git fetch -p; git fsck --full"
  },
  "dependencies": {
    "@fvilers/disable-react-devtools": "^1.3.0",
    "@mdi/font": "^7.4.47",
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@testing-library/user-event": "^14.6.1",
    "axios": "^1.8.4",
    "dompurify": "^3.2.4",
    "he": "^1.2.0",
    "javascript-obfuscator": "^4.1.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.4.1",
    "serve": "^14.2.4",
    "three": "^0.177.0",
    "three-stdlib": "^2.36.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/three": "^0.177.0",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vitest": "^3.0.9"
  }
}
